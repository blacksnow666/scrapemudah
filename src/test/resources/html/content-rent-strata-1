function twittershare(){
    var text = '@mudah Permai Puteri Apartment , Ampang, Selangor %0A' + encodeURIComponent("http://www.mudah.my/Permai+Puteri+Apartment+Ampang+Selangor-28711719.htm");
    var randomnumber=Math.floor(Math.random()*99999999);
    var twittersharer = 'https://twitter.com/share?url='+"http://www.mudah.my/Permai+Puteri+Apartment+Ampang+Selangor-28711719.htm"+'&text='+text+'&rand='+randomnumber;
    window.open(twittersharer,'twittersharer','width=600,height=350,toolbar=0,resizable=0');
}

function facebookshare(){
    var randomnumber=Math.floor(Math.random()*99999999);
    var fbsharer = '//www.facebook.com/sharer.php?u='+"http://www.mudah.my/Permai+Puteri+Apartment+Ampang+Selangor-28711719.htm"+'&ref=facebook&rand=' + randomnumber;
    window.open(fbsharer,'fbsharer','width=600,height=350,toolbar=0,resizable=0');
}

function googleshare(){
    var randomnumber=Math.floor(Math.random()*99999999);
    var googlesharer = 'https://plus.google.com/share?url='+"http://www.mudah.my/Permai+Puteri+Apartment+Ampang+Selangor-28711719.htm";
    window.open(googlesharer,'googlesharer','width=600,height=350,toolbar=0,resizable=0');
}
