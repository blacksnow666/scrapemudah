<script language="JavaScript" type="text/javascript">
	/* <![CDATA[ */
	var google_ads_load = false;
	var radlinks=0;

	function google_afs_request_done(google_ads) {
		if (google_ads.length > 0) {
			printResult(google_ads);
		}
		
		else {
			radlinks = 1;
			display_radlinks();
		}
		
		
	}

	function google_ad_request_done(google_ads) {
		if (!radlinks) {
			printResult(google_ads);
		}
	}

	function google_radlink_request_done(radlinks) {
		var s = '';
		if (radlinks.length) { 
			s += '<tr class="google_listing_ad" style="border-top:1px solid #ccc;border-bottom:0px;">';
			s += '<td colspan="7" style="padding-left:10px;">Related Ads<br>';
			s += '<table border="0" cellpadding="0" cellspacing="0" width="100%"><tr height="10px">';
			var ctr=0;
			for (var i=0; i<radlinks.length; i++) {
				if (ctr == 4) {
					s += '</tr><tr height="10px">';
					ctr = 0;
				}
				s += '<td style="height:10px;line-height:12px;border-bottom:0px;"><a href="http://www.mudah.my/relatedads.htm?ca=8_s&kw=' +
				     radlinks[i].url_escaped_term + '&rt=' + radlinks[i].radlink_token + '&url=' + encodeURIComponent(window.location) +
				     '">' + radlinks[i].term + '</a></td>';
				ctr++;
			}
			s += '</tr></table></td></tr>';
			document.write(s);
		}
	}

	function display_radlinks() { 
		google_ad_client = 'ca-scm-mudah-radlinks_js';
		google_ad_channel = 'radlinks_bottom_list_ads';
		google_ad_output = 'js';
		google_safe = 'high';
		google_adtest = 'off';
		google_num_radlinks = '12';
		google_max_radlink_len = '35';
		google_max_num_ads = '0';
		document.write("<" + "script language='JavaScript' src='http://pagead2.googlesyndication.com/pagead/show_ads.js'" + ">" + "<" + "/script>");
	}

	function printResult(google_ads) { 
		var s = '';
		var i;

		
		if (google_ads.length == 0){ 
			return;
		} else {
			var google_ads_link = google_info.feedback_url;
			
			if ( google_ads_load == false ) {
				google_ads_load = true;

				line_separator = ' ';
				container = document.getElementById('top_adsense');

				s += '<a target="_blank" href="' + google_ads_link + '">&nbsp;Ads by Google</a>';

				for(i = 0; i < google_ads.length; ++i) {
					if (google_ads[0].type == "text") {
						var tag_a = '<a class="google_adsense" target="_blank" href="' + google_ads[i].url + '" ' + '>';

						s += '<tr><td style="font-size:0.8em;">' + tag_a +
						google_ads[i].line1 + '</a><br/>' +
						google_ads[i].line2 + 
						google_ads[i].line3 + '<br/>' +
						'<a id="adview-ads-by-google-url" target="_blank" href="'+google_ads[i].url+'" style="color:#000000;font-weight:normal">'+ google_ads[i].visible_url +'</a>' + '<br></td></tr>';
					}
				}
		
				document.write(s);
			}
			s = '';

			
			var google_ads_link = google_info.feedback_url;

			for(i = 0; i < google_ads.length; ++i) {
				if (google_ads[i].type == "flash") {
					s += '<object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000"' +
						' codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,0,0"' +
						' WIDTH="' + google_ads[i].image_width +
						'" HEIGHT="' + google_ads[i].image_height + '">' +
						'<PARAM NAME="movie" VALUE="' + google_ads[i].image_url + '">' +
						'<PARAM NAME="quality" VALUE="high">' +
						'<PARAM NAME="AllowScriptAccess" VALUE="never">' +
						'<EMBED src="' + google_ads[i].image_url +
						'" WIDTH="' + google_ads[i].image_width +
						'" HEIGHT="' + google_ads[i].image_height + 
						'" TYPE="application/x-shockwave-flash"' + 
						' AllowScriptAccess="never" ' + 
						' PLUGINSPAGE="http://www.macromedia.com/go/getflashplayer"></EMBED></OBJECT>'; 
				}
				else  if (google_ads[i].type == "image") {
					s += '<a href="' + google_ads[i].url +
						'" target="_blank" title="go to ' + google_ads[i].visible_url +
						'"><img border="0" src="' + google_ads[i].image_url +
						'"width="' + google_ads[i].image_width +
						'"height="' + google_ads[i].image_height + '"></a>';
				} 
				else if (google_ads[0].type == "html") {
					s += google_ads[0].snippet;
				}
			}

			if (google_ads[0].type != "text") {
				s += '<p align="left"><a style="font-size:7pt;" href="' + google_info.feedback_url + '" target="_blank">Ads by Google</a></p>';
			}
			
			
			document.write(s);
		}
		return;
	}

	
	

	
	
	
	
			
	
	google_ad_client = 'ca-scm-mudah_js';
	google_ad_output = 'js';
	google_ad_type = 'text';
	google_adtest = 'off';
	google_safe = 'high';

	google_max_num_ads = '3';
	

	google_hints = '';
	google_ad_channel = 'detail_properties,detail_properties_flat';
	google_feedback = 'on';
	document.write("<" + "script language='JavaScript' src='http://pagead2.googlesyndication.com/pagead/show_ads.js'" + ">" + "<" + "/script>");
	

	

</script>
