<!-- Google Adsense start -->
<script language="JavaScript" type="text/javascript">
	/* <![CDATA[ */
	var google_ads_load = false;
	var radlinks=0;

	function google_afs_request_done(google_ads) {
		if (google_ads.length > 0) {
			printResult(google_ads);
		}
		
		else {
			radlinks = 1;
			display_radlinks();
		}
		
		
	}

	function google_ad_request_done(google_ads) {
		if (!radlinks) {
			printResult(google_ads);
		}
	}

	function google_radlink_request_done(radlinks) {
		var s = '';
		if (radlinks.length) { 
			s += '<tr class="google_listing_ad" style="border-top:1px solid #ccc;border-bottom:0px;">';
			s += '<td colspan="7" style="padding-left:10px;">Related Ads<br>';
			s += '<table border="0" cellpadding="0" cellspacing="0" width="100%"><tr height="10px">';
			var ctr=0;
			for (var i=0; i<radlinks.length; i++) {
				if (ctr == 4) {
					s += '</tr><tr height="10px">';
					ctr = 0;
				}
				s += '<td style="height:10px;line-height:12px;border-bottom:0px;"><a href="http://www.mudah.my/relatedads.htm?ca=9_s&kw=' +
				     radlinks[i].url_escaped_term + '&rt=' + radlinks[i].radlink_token + '&url=' + encodeURIComponent(window.location) +
				     '">' + radlinks[i].term + '</a></td>';
				ctr++;
			}
			s += '</tr></table></td></tr>';
			document.write(s);
		}
	}

	function display_radlinks() { 
		google_ad_client = 'ca-scm-mudah-radlinks_js';
		google_ad_channel = 'radlinks_bottom_list_ads';
		google_ad_output = 'js';
		google_safe = 'high';
		google_adtest = 'off';
		google_num_radlinks = '12';
		google_max_radlink_len = '35';
		google_max_num_ads = '0';
		document.write("<" + "script language='JavaScript' src='http://pagead2.googlesyndication.com/pagead/show_ads.js'" + ">" + "<" + "/script>");
	}

	function printResult(google_ads) { 
		var s = '';
		var i;

		
		if (google_ads.length == 0){ 
			return;
		} else {
			var google_ads_link = google_info.feedback_url;
			
			for(i = 0; i < google_ads.length ; ++i) {
				s += '<div class="google_listing_ad">';
				s += '<ul>';
				s += '<li class="listing_thumbs_date"><a class="google_adsense" target="_blank" href="' + google_ads_link + '" target="_blank" style="text-decoration:none;color:#000000;font-weight:normal">Google Ads</a></li>';
				s += '<li class="listing_thumbs_image"></li>';
				s += '<li class="listing_ads_title"><a class="google_adsense" target="_blank" href="'+google_ads[i].url+'" class="google_first_line">'+google_ads[i].line1+'</a><br>'+google_ads[i].line2 +'&nbsp;'+ google_ads[i].line3+'</li>';
				s += '<li class="list_ads_cat" onclick="adsenseClick()"><div class="listing_thumbs_google_last"><a target="_blank" href="'+google_ads[i].url+'" class="google_adsense" style="text-decoration:none;color:#000000;font-weight:normal">'+ google_ads[i].visible_url +'</a></div></li>';
				s += '</ul>';
				s += '</div>';
				
				s += '<div id="footerline_google" class="clear adsense_footer"></div>';
			}
			
			document.write(s);
		}
		return;
	}

	
	

	
	
			
	
	google_ad_client = 'ca-scm-mudah_js';
	google_ad_output = 'js';
	google_ad_type = 'text';
	google_adtest = 'off';
	google_safe = 'high';

	
	google_max_num_ads = '3';
	

	google_hints = '';
	google_ad_channel = 'listing_properties';
	google_feedback = 'on';
	document.write("<" + "script language='JavaScript' src='http://pagead2.googlesyndication.com/pagead/show_ads.js'" + ">" + "<" + "/script>");
	

	

</script>
<!-- Google Adsense end -->

